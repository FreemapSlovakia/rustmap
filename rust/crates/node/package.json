{
  "name": "maprender-node",
  "version": "0.1.0",
  "private": true,
  "description": "Node.js bindings for the maprender core renderer.",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "maprender_node.*.node"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "napi build --release",
    "build:debug": "napi build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.5.0",
    "@types/node": "^25.0.3"
  },
  "napi": {
    "binaryName": "maprender_node"
  }
}
