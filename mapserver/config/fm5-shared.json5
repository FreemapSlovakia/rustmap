{
  postgresConnectionString: "postgres://freemap:freemap4ever@localhost/freemap",
  dirs: {
    hillshading: "/fm/data2/hillshading",
    svg: "/home/freemap/X/rustmap/rust/images",
    tiles: "/fm/data4/X-tiles",
    expires: "/home/freemap/X/expires",
  },
  limits: {
    maxZoom: 20,
    polygon: "/home/freemap/X/europe-ok.geojson",
    scales: [1, 2, 3],
  },
  mapFeatures: {
    contours: true,
    shading: true,
    hikingTrails: true,
    bicycleTrails: true,
    skiTrails: true,
    horseTrails: true,
  },
  format: {
    extension: "jpeg",
    mimeType: "image/jpeg",
  },

  // TODO investigate if used in both:

  minExpiredBatchSize: 500,
  expiresZoom: 14, // imposm3 is configured to check for expired tiles on zoom 14
  prerenderMaxZoom: 14, // we need to know it even in on-demand only instance for creating index files
  // how long to delay rendering a tile when expiring tiles
  // this is to prevent drastical i/o slowdown when prerendering eats 100% cpu
  // set to zero to prevent this feature
  prerenderDelayWhenExpiring: 200,
}
